{
    "name": "@llmembed/embedjs",
    "version": "0.0.20",
    "description": "A NodeJS framework to easily work with LLMs and custom datasets",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/llmembed/embedjs"
    },
    "keywords": [
        "ai",
        "ml",
        "llm",
        "gpt",
        "openai",
        "chatgpt",
        "embedding",
        "embeddings",
        "openai",
        "langchain",
        "prompt",
        "vector"
    ],
    "license": "Apache-2.0",
    "author": "K V Adhityan",
    "bugs": {
        "url": "https://github.com/llmembed/embedjs/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/**/*"
    ],
    "homepage": "https://github.com/llmembed/embedjs#readme",
    "dependencies": {
        "axios": "^1.4.0",
        "html-to-text": "^9.0.5",
        "langchain": "^0.0.100",
        "md5": "^2.3.0",
        "pdf-parse-fork": "^1.2.0",
        "youtube-transcript": "^1.0.6"
    },
    "devDependencies": {
        "@types/node": "^20.3.2",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.8",
        "typescript": "^5.1.6",
        "@types/html-to-text": "^9.0.1"
    },
    "peerDependencies": {
        "@pinecone-database/pinecone": "^0.1.6",
        "lmdb": "^2.8.2",
        "vectordb": "^0.1.8",
        "chromadb": "^1.5.3",
        "hnswlib-node": "^1.4.2",
        "cohere-ai": "^5.1.0"
    },
    "peerDependenciesMeta": {
        "@pinecone-database/pinecone": {
            "optional": true
        },
        "vectordb": {
            "optional": true
        },
        "chromadb": {
            "optional": true
        },
        "lmdb": {
            "optional": true
        },
        "cohere-ai": {
            "optional": true
        },
        "hnswlib-node": {
            "optional": true
        }
    },
    "engines": {
        "node": ">= 18.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./databases/lance": {
            "types": "./dist/databases/lance-db.d.ts",
            "import": "./dist/databases/lance-db.js"
        },
        "./databases/pinecone": {
            "types": "./dist/databases/pinecone-db.d.ts",
            "import": "./dist/databases/pinecone-db.js"
        },
        "./databases/chroma": {
            "types": "./dist/databases/chroma-db.d.ts",
            "import": "./dist/databases/chroma-db.js"
        },
        "./databases/hnswlib": {
            "types": "./dist/databases/hnswlib-db.d.ts",
            "import": "./dist/databases/hnswlib-db.js"
        },
        "./cache/lmdb": {
            "types": "./dist/databases/lmdb-cache.d.ts",
            "import": "./dist/databases/lmdb-cache.js"
        },
        "./cache/memory": {
            "types": "./dist/databases/memory-cache.d.ts",
            "import": "./dist/databases/memory-cache.js"
        },
        "./embeddings/ada": {
            "types": "./dist/embeddings/ada-embeddings.d.ts",
            "import": "./dist/embeddings/ada-embeddings.js"
        },
        "./embeddings/cohere": {
            "types": "./dist/embeddings/cohere-embeddings.d.ts",
            "import": "./dist/embeddings/cohere-embeddings.js"
        },
        "./package.json": "./package.json"
    }
}
